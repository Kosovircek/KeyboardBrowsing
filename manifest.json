{

  "manifest_version": 3,
  "name": "KeyboardBrowsing",
  "version": "2.0",

  "description": "Allows for keyboard only web browsing",

  "icons": {
    "48": "icons/keyboard_browsing_icon.png"
  },

  "content_scripts": [
    {
	  "matches": ["<all_urls>"],
      "js": ["keyboardBrowsing.js"]
    }
  ]
}
